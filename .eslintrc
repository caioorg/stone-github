{
  "extends": ["react-app", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "warn",
    "global-require": 0,
    "class-methods-use-this": 0,
    "indent": ["error", 2],
    "semi": ["error", "never"],
    "comma-dangle": ["error", "never"],
    "object-curly-newline": 0,
    "no-mixed-operators": 0,
    "arrow-parens": 0,
    "no-return-assign": 0
  }
}
